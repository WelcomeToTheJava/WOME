package movimiento;

import java.awt.image.BufferedImage;

import entidades.Animacion;

public class DiagonalInferiorIzquierda implements Movimiento{
	private Animacion animacion;
	
	public DiagonalInferiorIzquierda(int velMovimiento, BufferedImage [] mov) {
		this.animacion = new Animacion(velMovimiento, mov);
	}
	
	@Override
	public BufferedImage movimiento() {
		return animacion.getFrameActual();
	}

	@Override
	public Animacion getAnimacion() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void reset() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public int actualizarPosicion() {
		// TODO Auto-generated method stub
		return 0;
	}
}
