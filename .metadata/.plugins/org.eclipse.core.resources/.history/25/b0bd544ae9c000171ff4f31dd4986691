package movimiento;

import java.awt.image.BufferedImage;

import entidades.Animacion;

public class DiagonalInferiorDerecha implements Movimiento {
	private Animacion animacion;
	private int velMovimiento;
	
	public DiagonalInferiorDerecha(int velMovimiento, Animacion animacion) {
		this.animacion = animacion;
		this.velMovimiento = velMovimiento;
	}
	
	@Override
	public BufferedImage movimiento() {
		return animacion.getFrameActual();
	}
}
